apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  name: sql-engine-servicemonitor
  labels:
    release: rancher-monitoring
spec:
  selector:
    matchLabels:
      app: spark-sql-engine
  endpoints:
    - port: metrics-port
      interval: 30s
      path: /metrics
      params:
        format: [prometheus]
  namespaceSelector:
    matchNames:
      - default


